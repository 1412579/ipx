{
  "name": "ipx",
  "version": "0.4.6",
  "repository": "nuxt-contrib/ipx",
  "license": "MIT",
  "exports": {
    "client": {
      "require": "./lib/client.js",
      "import": "./lib/client.es.js"
    },
    "entrypoint": {
      "require": "./lib/index.js",
      "import": "./lib/index.es.js"
    }
  },
  "main": "lib/index.cjs.js",
  "bin": "bin/cli.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "rimraf lib && siroc build",
    "dev": "nodemon",
    "lint": "eslint --ext .ts .",
    "release": "standard-version && yarn build && git push --follow-tags origin master && npm publish",
    "start": "node bin/ipx.js"
  },
  "dependencies": {
    "allowlist": "^0.1.1",
    "connect": "^3.7.0",
    "consola": "^2.15.0",
    "cron": "^1.8.2",
    "defu": "^3.2.2",
    "etag": "^1.8.1",
    "fs-extra": "^9.0.1",
    "is-valid-path": "^0.1.1",
    "node-fetch": "^2.6.1",
    "recursive-readdir": "^2.2.2",
    "sharp": "^0.27.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/connect": "latest",
    "@types/cron": "latest",
    "@types/etag": "latest",
    "@types/fs-extra": "latest",
    "@types/is-valid-path": "latest",
    "@types/node-fetch": "latest",
    "@types/recursive-readdir": "latest",
    "@types/sharp": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "nodemon": "latest",
    "rollup-plugin-typescript2": "latest",
    "siroc": "latest",
    "standard-version": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  }
}
